<script>
  const addToWishlist = async (productId, cartId) => {

    const response = await fetch(`/addToWishlist?productId=${productId}&cartId=${cartId}`, {
      method: 'GET',
      headers: {
        "Content-Type": "application/json",
      },
    })

    const data = await response.json()

    if (data.message === "Added to wishlist") {

      Swal.fire({
        position: "center",
        icon: "success",
        title: "Product is moved to wishlist",
        showConfirmButton: true,
        confirmButtonColor: "#00A300",
      });

      document.getElementById('row' + productId).innerHTML = ''

    } else {
      Swal.fire({
        position: "center",
        icon: "warning",
        title: "Product is already in wishlist!",
        showConfirmButton: true,
        confirmButtonColor: "#00A300",
      });
    }
  }

  async function submitReview() {
    // Retrieve the values entered by the user
    const author = document.getElementById('author').value
    const review = document.getElementById('review').value
    const product = document.getElementById('productId11').getAttribute('data-value');
    const dataone = {
      author: author,
      review: review,
      product: product
    };
    console.log(dataone)
    const respond = await fetch('/addNewReview', {
      method: 'POST',
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({
        data: dataone,
      }),
    })
    const feedback = await respond.json()
    if (feedback.message === "The review has been added successfully") {

      Swal.fire({
        position: "center",
        icon: "success",
        title: "The review has been added successfully",
        showConfirmButton: true,
        confirmButtonColor: "#00A300",
      });

      document.getElementById('row' + productId).innerHTML = ''

    } else if (feedback.message === "The user has already written the review") {
      Swal.fire({
        position: "center",
        icon: "warning",
        title: "The user has already written the review",
        showConfirmButton: true,
        confirmButtonColor: "#00A300",
      });
    }
    else if (feedback.message === "Please log in to add a review") {
      Swal.fire({
        position: "center",
        icon: "warning",
        title: "Please log in to add a review",
        showConfirmButton: true,
        confirmButtonColor: "#00A300",
      });
    } else {
      Swal.fire({
        position: "center",
        icon: "warning",
        title: "The user has not yet purchased this product",
        showConfirmButton: true,
        confirmButtonColor: "#00A300",
      });
    }
  }
  // Get the link element
  const link = document.getElementById("myLink");

  // Add a click event listener
  link.addEventListener("click", function (event) {
    event.preventDefault(); // Prevent the default link behavior

    // Add the CSS class to trigger the animation
    link.classList.add("animate-link");

    // Remove the CSS class after the animation completes
    setTimeout(function () {
      link.classList.remove("animate-link");
    }, 500); // Adjust the timeout duration to match the animation duration
  });

</script>




